      <targetParameter name="versionDate" value="n/a"/>
      <targetParameter name="producer" value="interactive instruments GmbH"/>
      <targetParameter name="xsltPath" value="sc-resources/xslt"/>
    </Target>-->
  </targets>
</ShapeChangeConfiguration>
