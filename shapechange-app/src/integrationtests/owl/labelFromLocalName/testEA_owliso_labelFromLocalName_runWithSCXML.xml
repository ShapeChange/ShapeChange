  <RdfTypeMapEntry rule="*" target="gsp:Feature" targetType="class" type="GFI_Feature"/>
 
</rdfMapEntries>
  
</rdfMapEntries>

			<rdfConversionParameters>
				<PropertyConversionParameter global="true" property="FT1::att" rule="*" schema="AppSchema"/>
