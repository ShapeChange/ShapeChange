</rdfMapEntries>
  <rdfMapEntries xmlns="http://www.interactive-instruments.de/ShapeChange/Configuration/1.1">
  
 <RdfTypeMapEntry rule="*" target="gsp:Geometry" targetType="class" type="DirectPosition"/>
