		<Transformer class="de.interactive_instruments.ShapeChange.Transformation.Adding.CodeListLoader" id="TRF_CL_LOADER" input="IDENTITY" mode="enabled">
			<rules>
				<ProcessRuleSet name="cl_loader_rules">
